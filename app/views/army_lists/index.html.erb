<h1>Listing army_lists</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Player</th>
  </tr>

<% @army_lists.each do |army_list| %>
  <tr>
    <td><%=h army_list.name %></td>
    <td><%=h army_list.player %></td>
    <td><%= link_to 'Show', army_list %></td>
    <td><%= link_to 'Edit', edit_army_list_path(army_list) %></td>
    <td><%= link_to 'Destroy', army_list, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New army_list', new_army_list_path %>