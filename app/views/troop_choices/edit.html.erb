<h1>Editing troop_choice</h1>

<% form_for(@troop_choice) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @troop_choice %> |
<%= link_to 'Back', troop_choices_path %>