<h1>Army lists</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Player</th>
  </tr>
  <% @army_lists.each do |army_list| %>
  <tr>
    <td><%=h army_list.name %></td>
    <td><%=h army_list.player %></td>
    <td><%= link_to 'Edit', :action => :edit, :id => army_list %></td>
    <td><%= link_to 'Destroy', army_list, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
  <% end %>
</table>

<%= link_to "New army list", new_army_list_path %>